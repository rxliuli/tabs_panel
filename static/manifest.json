{
  "name": "tabs_panel",
  "version": "1.0.1",
  "description": "一个标签页快速查找器",
  "permissions": ["tabs", "activeTab"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js", "reload.js"]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+E",
        "mac": "Command+E",
        "chromeos": "Ctrl+E",
        "linux": "Ctrl+E"
      }
    }
  },
  "homepage_url": "http://rxliuli.com/",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "image/icon/icon-popup-16.png",
    "32": "image/icon/icon-popup-32.png",
    "48": "image/icon/icon-popup-48.png",
    "128": "image/icon/icon-popup-128.png"
  },
  "manifest_version": 2
}
