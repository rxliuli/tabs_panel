{
  "name": "tabs_panel",
  "version": "1.0",
  "description": "一个标签页快速查找器",
  "permissions": ["tabs", "activeTab", "storage"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js", "reload.js"],
    "persistent": false
  },
  "commands": {
    "showPopupPanel": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      },
      "description": "显示扩展面板"
    }
  },
  "homepage_url": "http://rxliuli.com/",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
