{
  "name": "tabs_panel",
  "version": "1.0.2",
  "description": "一个标签页快速查找器",
  "permissions": ["tabs", "activeTab"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js", "reload.js"]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+A",
        "mac": "Alt+A",
        "chromeos": "Alt+A",
        "linux": "Alt+A"
      }
    }
  },
  "homepage_url": "http://github.com/rxliuli/tabs_panel",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "image/icon/icon-popup-16.png",
    "32": "image/icon/icon-popup-32.png",
    "48": "image/icon/icon-popup-48.png",
    "128": "image/icon/icon-popup-128.png"
  },
  "manifest_version": 2
}
